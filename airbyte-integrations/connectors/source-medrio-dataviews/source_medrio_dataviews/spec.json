{
  "documentationUrl": "https://medrio-docs.atlassian.net/wiki/spaces/MD/pages/1341128715/Data+Views+API+Reference",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Medrio Dataviews Spec",
    "type": "object",
    "required": [
      "api_user_name",
      "api_user_password",
      "enterprise_api_key"
    ],
    "additionalProperties": false,
    "properties": {
      "api_user_name": {
        "type": "string",
        "description": "The username for your Medrio api user."
      },
      "api_user_password": {
        "type": "string",
        "description": "The password for your Medrio api user."
      },
      "enterprise_api_key": {
        "type": "string",
        "description": "An API key with Enterprise-level access."
      }
    }
  }
}